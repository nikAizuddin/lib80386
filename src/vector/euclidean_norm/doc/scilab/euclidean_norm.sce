function[] = euclidean_norm()

    // Source matrix
    A = [-32.4012   42.1234  -85.5213
          90.3214  -49.123   -64.2356
          84.1235   95.2314   33.1456
         -90.3213  -42.2391  -63.3524
          52.2014  -69.2405   64.3777];

    B = norm(A(1,:));
    C = norm(A(:,1));
    D = norm(A(3,:));
    E = norm(A(:,2));

    disp("B =");disp(B);
    disp("C =");disp(C);
    disp("D =");disp(D);
    disp("E =");disp(E);

endfunction